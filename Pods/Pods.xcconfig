PODS_ROOT = ${SRCROOT}/Pods
ALWAYS_SEARCH_USER_PATHS = YES
OTHER_LDFLAGS = -ObjC -framework Foundation -framework OpenGLES -framework OpenAL -framework AVFoundation -framework AudioToolbox -framework QuartzCore -framework GameKit -lz
HEADER_SEARCH_PATHS = "${PODS_ROOT}/cocos2d/external/kazmath/include" "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/TouchJSON" "${PODS_ROOT}/Headers/cocos2d" "${PODS_ROOT}/Headers/cocos2d/Platforms" "${PODS_ROOT}/Headers/cocos2d/Platforms/iOS" "${PODS_ROOT}/Headers/cocos2d/Platforms/Mac" "${PODS_ROOT}/Headers/cocos2d/Support" "${PODS_ROOT}/Headers/CocosDenshion/CocosDenshion" "${PODS_ROOT}/Headers/external/libpng" "${PODS_ROOT}/Headers/external/kazmath/include/kazmath" "${PODS_ROOT}/Headers/external/kazmath/include/kazmath/GL"